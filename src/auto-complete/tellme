_tellme() 
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"
    opts1="about"
    opts2="my"
    opts3="system os hardware arch"

    if [[ ! ${COMP_WORDS[*]} =~ about ]] && [[ ${cur} == * ]] ; then
        COMPREPLY=( $(compgen -W "${opts1}" -- ${cur}) )
        return 0
    fi

    if [[ ! ${COMP_WORDS[*]} =~ system ]] &&
        [[ ! ${COMP_WORDS[*]} =~ os ]] &&
        [[ ! ${COMP_WORDS[*]} =~ hardware ]] &&
        [[ ! ${COMP_WORDS[*]} =~ arch ]] &&
        [[ ${cur} != m* ]] &&
        [[ ${cur} == * ]]; then
        COMPREPLY=( $(compgen -W "${opts3}" -- ${cur}) )
        return 0
    fi
    
    if [[ ! ${COMP_WORDS[*]} =~ my ]] && ([[ ${cur} == m* ]] || [[ ${cur} == * ]]); then
        COMPREPLY=( $(compgen -W "${opts2}" -- ${cur}) )
        return 0
    fi

    if [[ ! ${COMP_WORDS[*]} =~ system ]] && [[ ${cur} == s* ]]; then
        COMPREPLY=( $(compgen -W "${opts2}" -- ${cur}) )
        return 0
    fi

    if [[ ! ${COMP_WORDS[*]} =~ os ]] && [[ ${cur} == o* ]]; then
        COMPREPLY=( $(compgen -W "${opts2}" -- ${cur}) )
        return 0
    fi

    if [[ ! ${COMP_WORDS[*]} =~ hardware ]] && [[ ${cur} == h* ]]; then
        COMPREPLY=( $(compgen -W "${opts2}" -- ${cur}) )
        return 0
    fi

    if [[ ! ${COMP_WORDS[*]} =~ arch ]] && [[ ${cur} == a* ]]; then
        COMPREPLY=( $(compgen -W "${opts2}" -- ${cur}) )
        return 0
    fi
}
complete -F _tellme tellme